function pickup_update()		
		for p in all(pu) do 
				if aabb_collide(player.x,
							player.y,player.w,	
							player.h,p.x * 8, 
							p.y * 8,6,6)
		 	then
		 			sfx(0)
		 			player.coin_cnt+=1
		 			player.score+=100
						del(pu,p)
				end
		end
end

function pickup_draw()
		camera(cam_x, cam_y)
		
		for p in all(pu) do
		 	spr(p.s,p.x*8,p.y*8)
		end
		
		camera(0,0)
end




