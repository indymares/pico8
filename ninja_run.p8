function aabb_collide(
											x1,y1,w1,h1,
											x2,y2,w2,h2)
		
		if x1 < x2 + w2 and
		--checked outside top corners
					x2 < x1 + w1 and
		--checked inside top corners
					y1 < y2 + h2 and
		--checked outside side corners
					y2 < y1 + h1
		--checked inside side corners
		then
					return true
		end 
					return false
		
end

function score_draw()
		print("coins:"..player.coin_cnt.."/20",
								3,0,11)
end

function time_left()
		cnt-=1
		if cnt<0 then 
				player.timer-=1
				if player.timer<11 then
						music(5)
				end
				if player.timer<1 then
						music(-1)
						sfx(1)
						scene="dead"
				end
				cnt=30
		end
		print("time left:"..player.timer,
								3,8,11)
end
