--main
function _init()
		x=60
		y=60
		s=1
end

function _update()
		pl_update()
end

function _draw()
  cls()
  map(0,0)
  spr(s,x,y)
end
-->8
--player move

function pl_update()
		if btn(⬆️) then
				y-=1
		end
		
		if btn(⬇️) then
				y+=1
		end
		
		if btn(⬅️) then
				x-=1
		end
		
		if btn(➡️) then
				x+=1
		end
end
